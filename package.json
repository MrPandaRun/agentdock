{
  "name": "agentdock",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev:desktop": "pnpm --filter @agentdock/desktop tauri dev",
    "dev:mobile": "pnpm --filter @agentdock/mobile start",
    "dev": "pnpm --filter @agentdock/desktop run dev",
    "build": "pnpm -r --if-present run build",
    "lint": "pnpm -r --if-present run lint",
    "typecheck": "pnpm -r --if-present run typecheck && pnpm rust:check",
    "test": "pnpm -r --if-present run test && pnpm rust:test",
    "rust:check": "cargo check --workspace --manifest-path ./Cargo.toml",
    "rust:test": "cargo test --workspace --manifest-path ./Cargo.toml"
  }
}
